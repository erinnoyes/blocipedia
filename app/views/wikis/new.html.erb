<div class="container">
  <div class="row">
  <h1>New Wiki</h1>
  <div class="col-md-8">
    <%= form_for @wiki do |f| %>
    <div class="form-group">
      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :body %>
      <%= f.text_area :body, class: 'form-control' %>
    </div>
    <%= render partial: 'form', locals: {f: f} %>
      <%= f.submit "Save" %>
    <% end %>
  </div>
</div>
